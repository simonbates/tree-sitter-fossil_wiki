{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fossil_wiki",
      "camelcase": "FossilWiki",
      "title": "Fossil Wiki",
      "scope": "source.fossil_wiki",
      "file-types": [
        "wiki"
      ],
      "injection-regex": "^fossil_wiki$",
      "class-name": "TreeSitterFossilWiki"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "FossilWiki grammar for tree-sitter",
    "authors": [
      {
        "name": "Simon Bates",
        "email": "simon@bitstructures.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-fossil_wiki"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}